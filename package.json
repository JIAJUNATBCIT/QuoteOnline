{
  "name": "quote-online",
  "version": "1.0.0",
  "description": "全栈在线询价系统",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev:server": "npm run server",
    "dev:client": "npm run client",
    "prod": "npm run build:prod && npm run start:prod",
    "prod:build": "npm run build:prod",
    "test": "npm run start",
    "server": "nodemon server.js",
    "server:no-warn": "NODE_NO_WARNINGS=1 nodemon server.js",
    "client": "cd client && ng serve",
    "build": "cd client && ng build",
    "build:prod": "cd client && ng build --configuration production",
    "install-all": "npm install && cd client && npm install",
    "logs": "node scripts/view-logs.js",
    "logs:error": "node scripts/view-logs.js --error",
    "logs:tail": "node scripts/view-logs.js --tail",
    "logs:today": "node scripts/view-logs.js",
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "iconv-lite": "^0.7.0",
    "jsonwebtoken": "^9.0.2",
    "mailgun.js": "^12.4.0",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "karma-chrome-launcher": "^3.2.0",
    "nodemon": "^3.0.2"
  },
  "author": "",
  "license": "ISC"
}
